  "dependencies": {
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@forgedev-br/eslint-config": "^2.0.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.5.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.4",
    "vite": "^5.4.2",
    "vue-tsc": "^2.0.29"
  }
